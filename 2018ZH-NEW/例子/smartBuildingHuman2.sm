ctmc

module human

s : [0..2] init 0;
humanNum : [0..200] init 0;
eventIndicator_1 : [0..1] init 0;
eventIndicator_2 : [0..1] init 0;

[] s=0 & humanNum>0 -> 4 : (s'=2)  & (humanNum'=humanNum-1) &(eventIndicator_2'=1);
[] s=0 & humanNum<100 -> 5 : (s'=1)  & (humanNum'=humanNum+1)&(eventIndicator_1'=1);
[] s=2 -> (s'=0)  & (eventIndicator_2'=0);
[] s=1 -> (s'=0)  & (eventIndicator_1'=0);

endmodule
